<% unless asks.nil? or asks.empty? %>
<table class="requesteditems">
    <tr>
        <th>Up for Grabs</th>
    </tr>
  <% asks.each do |ask| %>
    <tr class="<%= $state[ask.state] %>" id="<%= ask._id %>">
        <td><%= erb :_ask, :layout => false, :locals => { :ask => ask } %></td>
    </tr>
  <% end %>
</table>
<% end %>
